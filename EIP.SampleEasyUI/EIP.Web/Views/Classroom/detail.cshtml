@model EIP.Entity.Classroom

@{
    Layout = "~/views/shared/_PersonCenterLayout.cshtml";
}
@section head{
    <link href="/Content/PersonalCenter/CSS/table.css?v=@ViewBag.Version" rel="stylesheet" />
}
<div>
    <div class="div-margin">
        <input type="button" id="Colse" class="form-btn" onclick="closeBox();" value="关闭" />
        
    </div>
    <div class="head-font">
        <b>教室信息
   详情</b>
    </div>

    <table id="readonly" class="tables" cellpadding="0" cellspacing="0">
            
                <tr>
                    <td width="25%">
                        <label>教室信息Id</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.ClassroomId)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>教室名</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.ClassroomName)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>备注</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Other)
                    </td>
                </tr>
            
    </table>
</div>

<script type="text/javascript">

    mini.parse();


    //关闭
    function closeBox() {

        window.App.CloseWindow();
    }
</script>