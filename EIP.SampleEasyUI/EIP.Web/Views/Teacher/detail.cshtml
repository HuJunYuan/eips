@model EIP.Entity.eacher

@{
    Layout = "~/views/shared/_PersonCenterLayout.cshtml";
}
@section head{
    <link href="/Content/PersonalCenter/CSS/table.css?v=@ViewBag.Version" rel="stylesheet" />
}
<div>
    <div class="div-margin">
        <input type="button" id="Colse" class="form-btn" onclick="closeBox();" value="关闭" />
        
    </div>
    <div class="head-font">
        <b>老师表详情</b>
    </div>

    <table id="readonly" class="tables" cellpadding="0" cellspacing="0">
            
                <tr>
                    <td width="25%">
                        <label>老师ID</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.TeacherID)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>老师名字</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.TeacherName)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>老师性别(sex)</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Sex)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>老师年龄(age)</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Age)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>教学科目(subject)</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Subject)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label></label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.HomeAddress)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label></label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.Phone)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>学生老师关系表(StudentTeacherRelationship)</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.StudentTeacherRelationships)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>老师班级表关系(TeacherClassRelationship)</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.eacherClassRelationships)
                    </td>
                </tr>
            
    </table>
</div>

<script type="text/javascript">

    mini.parse();


    //关闭
    function closeBox() {

        window.App.CloseWindow();
    }
</script>