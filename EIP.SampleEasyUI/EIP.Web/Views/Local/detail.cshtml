@model EIP.Entity.Local

@{
    Layout = "~/views/shared/_PersonCenterLayout.cshtml";
}
@section head{
    <link href="/Content/PersonalCenter/CSS/table.css?v=@ViewBag.Version" rel="stylesheet" />
}
<div>
    <div class="div-margin">
        <input type="button" id="Colse" class="form-btn" onclick="closeBox();" value="关闭" />
        
    </div>
    <div class="head-font">
        <b>用来记录地域相关属性详情</b>
    </div>

    <table id="readonly" class="tables" cellpadding="0" cellspacing="0">
            
                <tr>
                    <td width="25%">
                        <label>记录地域表中的编号</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.LocalID)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>记录地域编号，类似邮编</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.LocalNum)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>记录地域的中文名</label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.LocalName)
                    </td>
                </tr>
            
            
                <tr>
                    <td width="25%">
                        <label>当前地域记录的父级LocalID
   </label>
                    </td>
                    <td>
                        @Html.DisplayFor(p => p.ParentId)
                    </td>
                </tr>
            
    </table>
</div>

<script type="text/javascript">

    mini.parse();


    //关闭
    function closeBox() {

        window.App.CloseWindow();
    }
</script>