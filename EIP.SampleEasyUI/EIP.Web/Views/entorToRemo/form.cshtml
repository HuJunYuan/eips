@model EIP.Entity.entorToRemo

@{
    Layout = null;
}
@section head{
}
<form id="form_entorToRemo" class="easyui-form" method="post" data-options="">
        
            <!--班级任课教师数据ID-->
            @Html.HiddenFor(p => p.MTRId)    
        
            <!--任课教师Id-->
            @Html.HiddenFor(p => p.MId)    
        
            <!---->
            @Html.HiddenFor(p => p.RId)    

        
            <div class="form-item">
                    @Html.CLTextBoxFor(p => p.Other, new TextBox() { Label = "备注
    ：", Style = "width:90%;" })
            </div>

        
            <div class="form-item">
                    @Html.CLTextBoxFor(p => p.LessonSchedules, new TextBox() { Label = "课程安排 ：", Style = "width:90%;" })
            </div>

        
            <div class="form-item">
                    @Html.CLTextBoxFor(p => p.entor, new TextBox() { Label = "教师 ：", Style = "width:90%;" })
            </div>

        
            <div class="form-item">
                    @Html.CLTextBoxFor(p => p.Remo, new TextBox() { Label = "班级 ：", Style = "width:90%;" })
            </div>

</form>
<script type="text/javascript">

    function validate() {
        // 验证
        var isValid = $("#form_entorToRemo").form("validate");
        if (!isValid) {
            App.showMessageBox('@MessageManager.GetMessage("E10060")', 2);
            return false;
        }
        return true;
    }

    //保存
    function save() {

        // 表单验证
        if (!validate()) return;

        // 表单数据提交
        var formData = $("#form_entorToRemo").form("getData");
        App.action(App.getUrl("entorToRemo/save"), formData,
            function (data) {
                //todo
            });

    }

    //关闭
    function closeBox() {

        window.App.showConfirm('@MessageManager.GetMessage("Q10030")',
            function () {
                window.App.CloseWindow();
            });
    }
</script>
