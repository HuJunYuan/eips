    <script src="@Url.AutoVersioning("~/Scripts/jquery-easyui-1.5/plugins/jeasyui.extensions.ico.js")"></script>

    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()
    @RenderSection("footer", required: false)
    @Html.ShowMessageBox()
</body>
</html>
