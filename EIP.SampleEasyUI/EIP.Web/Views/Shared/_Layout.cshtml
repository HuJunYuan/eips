    <link href="@Url.AutoVersioning("~/Scripts/jquery-easyui-1.5/themes/icon.css")" rel="stylesheet" />
    <link href="@Url.AutoVersioning("~/Content/css/Common.css")" rel="stylesheet" />

    <script src="@Url.AutoVersioning("~/Scripts/jquery.min.js")"></script>
    <script src="@Url.AutoVersioning("~/Scripts/jquery.jdirk.js")"></script>
    <script src="@Url.AutoVersioning("~/Scripts/json2.js")"></script>
    <script src="@Url.AutoVersioning("~/Scripts/App.js")"></script>
